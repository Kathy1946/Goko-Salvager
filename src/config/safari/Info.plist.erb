<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Author</key>
    <string><%= config[:author] %></string>
	<key>Builder Version</key>
	<string>7536.30.1</string>
	<key>CFBundleDisplayName</key>
    <string><%= config[:title] %></string>
	<key>CFBundleIdentifier</key>
    <string>com.dominionstrategy.safari.<%= config[:name] %></string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleShortVersionString</key>
    <string><%= config[:version] %></string>
	<key>CFBundleVersion</key>
	<string><%= config[:version] %></string>
	<key>Chrome</key>
	<dict/>
	<key>Content</key>
	<dict>
		<key>Scripts</key>
		<dict>
			<key>End</key>
			<array>
                <% for js in config[:js] %>
                    <string><%= js %></string>
                <% end %>
			</array>
		</dict>
		<key>Stylesheets</key>
		<array>
            <% for css in config[:js] %>
                <string><%= css %></string>
            <% end %>
		</array>
		<key>Whitelist</key>
		<array>
            <% for url in config[:targeturls] %>
                <string><%= url %></string>
            <% end %>
		</array>
	</dict>
	<key>Description</key>
    <string><%= config[:desc] %></string>
	<key>ExtensionInfoDictionaryVersion</key>
	<string>1.0</string>
	<key>Permissions</key>
	<dict>
		<key>Website Access</key>
		<dict>
			<key>Allowed Domains</key>
			<array>
				<string>*.goko.com/Dominion</string>
			</array>
            <key>Include Secure Pages</key>
            <true/>
			<key>Level</key>
			<string>Some</string>
		</dict>
	</dict>
	<key>Website</key>
    <string><%= config[:homepage] %></string>
    <key>URL</key>
    <string><%= config[:safarixtz_url] %></string>

</dict>
</plist>
